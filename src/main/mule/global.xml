	<configuration-properties doc:name="Configuration properties" doc:id="79bc15de-7656-47c9-b2fb-173c73ea039e" file="${mule.env}.yaml" />
	<http:listener-config name="HTTP_Listener_config" doc:name="HTTP Listener config" doc:id="ae442cc8-62ba-4993-8f4d-d3db079c2679" >
		<http:listener-connection host="0.0.0.0" port="${http.port}" />
	</http:listener-config>
	<db:config name="Database_Config" doc:name="Database Config" doc:id="9784ae92-6673-41f4-957c-53e24467e7a7" >
		<db:my-sql-connection host="${database.host}" port="${database.port}" user="${database.username}" password="${database.password}" database="${database.dbName}" />
	</db:config>
	

</mule>
